<?xml version="1.0" standalone="yes"?>
<!-- define an atomType for Silicon in a forcefield calculation -->
<cml 
    xmlns="http://www.xml-cml.org/schema"
	 xsi:schemaLocation="http://www.xml-cml.org/schema ../../schema.xsd"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
    <bondType name="arch" convention="foo:foo" title="C-H bond on aromatic ring"/>
<!-- refer to it elsewhere -->
    <molecule>
        <atomArray/>
        <bondArray>
            <bond id="b2" atomRefs2="a1 a2" title="C.ar-H bond" order="S">
                <bondType ref="x:arch"/> 
            </bond>
        </bondArray>
    </molecule>
</cml>
